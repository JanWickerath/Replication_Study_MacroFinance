#! python


def build(ctx):
    ctx(
        features='run_m_script',
        source='Replication_Figure_1_updated.m',
        target=ctx.path_to(
            ctx, 'OUT_FIGURES', 'Replication_Figure_1_updated.pdf'
        ),
        deps=[
            ctx.path_to(ctx, 'OUT_DATA', 'data_figure_1.mat'),
        ],
        name='Figure_1_updated'
    )
